{
    "conf": {
        "spark": {
            "master": "local[*]",
            "others": {
                "spark.hadoop.validateOutputSpecs": "false",
                "spark.executor.memory": "4g"
            }
        }
    },
    "stages": {
        "grid_generation": {
            "conf": {
                "repartition": 12,
                "cache": false
            },
            "inputs": [
                {
                    "list": ["berlin"]
                }
            ],
            "outputs": [
                {
                    "path": "output_grid_generation"
                }
            ],
            "previous": [
                {
                    "name": "",
                    "executed": true,
                    "location": ""
                }
            ]
        },
        "grid_labeling": {
            "conf": {
                "repartition": 12
            },
            "inputs": [
                {
                    "path": "output_grid_generation"
                }
            ],
            "outputs": [
                {
                    "path": "output_grid_labeling"
                }
            ],
            "previous": [
                {
                    "name": "grid_generation"
                }
            ]
        }
    }
}
