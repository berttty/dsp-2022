{
    "conf": {
        "spark": {
            "master": "local[*]",
            "others": {
                "spark.hadoop.validateOutputSpecs": "false",
                "spark.executor.memory": "4g"
            }
        },
        "cities": ["berlin"]
    },
    "stages": {
        "grid_generation": {
            "conf": {
                "repartition": 12,
                "cache": false
            },
            "inputs": [
                {
                    "value": ".conf.cities",
                    "type": "key"
                }
            ],
            "outputs": [
                {
                    "value": "output_grid_generation",
                    "type": "file"
                }
            ],
            "previous": []
        },
        "grid_labeling": {
            "conf": {
                "repartition": 12
            },
            "inputs": [
                {
                    "value": "output_grid_generation",
                    "type": "file"
                }
            ],
            "outputs": [
                {
                    "value": "output_grid_labeling",
                    "type": "file"
                }
            ],
            "previous": [
                {
                    "name": "grid_generation"
                }
            ]
        },
        "clean_timeseries": {
            "conf": {
                "repartition": 12
            },
            "inputs": [
                {
                    "value": "file:///Users/bertty/dataset_dsp/Berlin/Rides/test/*",
                    "type": "file"
                }
            ],
            "outputs": [
                {
                    "value": "output_clean_timeseries",
                    "type": "file"
                }
            ],
            "previous": []
        },
        "tile_generation": {
            "conf": {
                "repartition": 12
            },
            "inputs": [
                {
                    "value": "output_clean_timeseries",
                    "type": "file"
                }
            ],
            "outputs": [
                {
                    "value": "output_tile_generation",
                    "type": "file"
                }
            ],
            "previous": [
                {
                    "name": "clean_timeseries"
                }
            ]
        },
        "tile_usage_calculation": {
            "conf": {
                "repartition": 12,
                "reduction": 0.10
            },
            "inputs": [
                {
                    "value": "output_tile_generation",
                    "type": "file"
                }
            ],
            "outputs": [
                {
                    "value": "output_tile_usage_calculation",
                    "type": "file"
                }
            ],
            "previous": [
                {
                    "name": "tile_generation"
                }
            ]
        }
    }
}
